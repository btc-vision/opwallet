    "name": "opwallet",
    "version": "1.3.2",
    "private": true,
    "type": "module",
    "homepage": "https://github.com/unisat-wallet/extension#readme",
    "bugs": {
        "url": "https://github.com/unisat-wallet/extension/issues"
